<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".Registrarse">

    <TextView
            android:text="Registrarse como Standard"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView3"
            android:textSize="20sp"
            app:layout_constraintBottom_toTopOf="@+id/textNombreS" android:layout_marginEnd="84dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="83dp" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="30dp"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/textNombreS"
            android:hint="Nombre"
            app:layout_constraintBottom_toTopOf="@+id/textApellidoS" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="98dp" android:layout_marginEnd="103dp"
            app:layout_constraintEnd_toEndOf="parent" tools:ignore="TouchTargetSizeCheck"
            app:layout_constraintTop_toBottomOf="@+id/textView3" android:layout_marginTop="25dp"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/textApellidoS"
            android:hint="Apellido"
            tools:ignore="TouchTargetSizeCheck"
            app:layout_constraintTop_toBottomOf="@+id/textNombreS" android:layout_marginEnd="102dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintBottom_toTopOf="@+id/textDireccionS"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="100dp"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/textDireccionS"
            android:hint="Dirección" app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="98dp"
            android:layout_marginEnd="103dp" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toTopOf="@+id/textCorreoS"
            tools:ignore="TouchTargetSizeCheck"
            app:layout_constraintTop_toBottomOf="@+id/textApellidoS" app:layout_constraintHorizontal_bias="0.0"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/textCorreoS"
            android:hint="Correo" app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="98dp"
            app:layout_constraintBottom_toTopOf="@+id/textTelefonoS"
            android:layout_marginEnd="103dp" app:layout_constraintEnd_toEndOf="parent"
            tools:ignore="TouchTargetSizeCheck"
            app:layout_constraintTop_toBottomOf="@+id/textDireccionS"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/textTelefonoS"
            android:hint="Teléfono" app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="98dp"
            app:layout_constraintBottom_toTopOf="@+id/textPosicionS"
            android:layout_marginEnd="103dp" app:layout_constraintEnd_toEndOf="parent"
            tools:ignore="TouchTargetSizeCheck"
            app:layout_constraintTop_toBottomOf="@+id/textCorreoS"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/textPosicionS"
            android:hint="Posición"
            app:layout_constraintBottom_toTopOf="@+id/textFechaS" android:layout_marginEnd="103dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="99dp" tools:ignore="TouchTargetSizeCheck"
            app:layout_constraintTop_toBottomOf="@+id/textTelefonoS" app:layout_constraintHorizontal_bias="0.0"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/textFechaS"
            android:hint="Fecha de inicio" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="98dp"
            app:layout_constraintBottom_toTopOf="@+id/textUsuarioS" android:layout_marginEnd="103dp"
            app:layout_constraintEnd_toEndOf="parent" tools:ignore="TouchTargetSizeCheck"
            app:layout_constraintTop_toBottomOf="@+id/textPosicionS"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/textUsuarioS"
            android:hint="Nombre de usuario" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="98dp" android:layout_marginEnd="103dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toTopOf="@+id/textPasswordS" tools:ignore="TouchTargetSizeCheck"
            app:layout_constraintTop_toBottomOf="@+id/textFechaS" app:layout_constraintHorizontal_bias="0.0"/>
    <EditText
            android:layout_width="215dp"
            android:layout_height="38dp"
            android:inputType="textPassword"
            android:ems="10"
            android:id="@+id/textPasswordS"
            android:hint="Contraseña" app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="100dp"
            android:layout_marginEnd="101dp" app:layout_constraintEnd_toEndOf="parent"
            tools:ignore="TouchTargetSizeCheck"
            app:layout_constraintTop_toBottomOf="@+id/textUsuarioS"/>
    <Button
            android:text="Registrarse"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/btnRegistrarseS"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="137dp"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginEnd="138dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginTop="70dp"
            app:layout_constraintTop_toBottomOf="@+id/textPasswordS" android:layout_marginBottom="50dp"/>
</androidx.constraintlayout.widget.ConstraintLayout>