<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".RegistrarUsuario">

    <TextView
            android:text="Registrarse como Standard"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView3"
            android:textSize="20sp" android:layout_marginBottom="32dp"
            app:layout_constraintBottom_toTopOf="@+id/textNombre" android:layout_marginEnd="84dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="83dp"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/textNombre"
            android:hint="Nombre" android:layout_marginBottom="14dp"
            app:layout_constraintBottom_toTopOf="@+id/textApellido" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="98dp" android:layout_marginEnd="103dp"
            app:layout_constraintEnd_toEndOf="parent" tools:ignore="TouchTargetSizeCheck"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/textApellido"
            android:hint="Apellido" android:layout_marginBottom="9dp"
            app:layout_constraintBottom_toTopOf="@+id/textDireccion" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="98dp" android:layout_marginEnd="103dp"
            app:layout_constraintEnd_toEndOf="parent" tools:ignore="TouchTargetSizeCheck"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/textDireccion"
            android:hint="Dirección" app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="98dp"
            android:layout_marginEnd="103dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginBottom="11dp" app:layout_constraintBottom_toTopOf="@+id/textCorreo"
            tools:ignore="TouchTargetSizeCheck"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/textCorreo"
            android:hint="Correo" app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="98dp"
            android:layout_marginBottom="15dp" app:layout_constraintBottom_toTopOf="@+id/textTelefono"
            android:layout_marginEnd="103dp" app:layout_constraintEnd_toEndOf="parent"
            tools:ignore="TouchTargetSizeCheck"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/textTelefono"
            android:hint="Teléfono" app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="98dp"
            android:layout_marginBottom="9dp" app:layout_constraintBottom_toTopOf="@+id/textPosicion"
            android:layout_marginEnd="103dp" app:layout_constraintEnd_toEndOf="parent"
            tools:ignore="TouchTargetSizeCheck"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/textPosicion"
            android:hint="Posición" android:layout_marginBottom="14dp"
            app:layout_constraintBottom_toTopOf="@+id/textFecha" android:layout_marginEnd="103dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="99dp" tools:ignore="TouchTargetSizeCheck"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/textFecha"
            android:hint="Fecha de inicio" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="98dp" android:layout_marginBottom="12dp"
            app:layout_constraintBottom_toTopOf="@+id/textUsuario" android:layout_marginEnd="103dp"
            app:layout_constraintEnd_toEndOf="parent" tools:ignore="TouchTargetSizeCheck"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/textUsuario"
            android:hint="Nombre de usuario" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="98dp" android:layout_marginEnd="103dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginBottom="7dp"
            app:layout_constraintBottom_toTopOf="@+id/textPassword" tools:ignore="TouchTargetSizeCheck"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPassword"
            android:ems="10"
            android:id="@+id/textPassword"
            android:hint="Contraseña" app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="100dp"
            android:layout_marginBottom="30dp" app:layout_constraintBottom_toTopOf="@+id/btnRegistrarse"
            android:layout_marginEnd="101dp" app:layout_constraintEnd_toEndOf="parent"
            tools:ignore="TouchTargetSizeCheck"/>
    <Button
            android:text="Registrarse"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/btnRegistrarse"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="137dp"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="54dp"
            android:layout_marginEnd="138dp" app:layout_constraintEnd_toEndOf="parent"/>
</androidx.constraintlayout.widget.ConstraintLayout>